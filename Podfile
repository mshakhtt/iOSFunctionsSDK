# platform :ios, '9.0'

target 'iOSfunctions' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iOSfunctions

  pod 'AFNetworking', '~> 4.0'
  pod 'RxSwift', '6.2.0'
  pod 'RxCocoa'
  pod 'RxDataSources'
  pod 'Connectivity'
